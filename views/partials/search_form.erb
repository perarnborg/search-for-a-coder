<section class="user-search clearfix">
  <form class="js-user-search">
  	<div class="searcher-type">
	    <div class="input-field">
	      <input type="radio" id="js-tech-no" class="js-tech js-tech-no tech" name="tech" value="not-techy" checked="chedked" /> 
	      <label for="js-tech-no">I am not a techy person</label>
	    </div>
	    <div class="input-field">
	      <input type="radio" id="js-tech-yes" class="js-tech tech" name="tech" value="techy" /> 
	      <label for="js-tech-yes">I am a techy person</label>
	    </div>
  	</div>
  	<div class="js-tech-fields tech-fields">
  	  <div class="search-question">
        <div class="input-field">
          <label for="js-language-css"><input type="radio" id="js-language-css" class="js-language" name="language" value="css" /> CSS</label>
        </div>
        <div class="input-field">
          <label for="js-language-javascript"><input type="radio" id="js-language-javascript" class="js-language" name="language" value="javascript" /> JavaScript</label>
        </div>
        <div class="input-field">
          <label for="js-language-ruby"><input type="radio" id="js-language-ruby" class="js-language" name="language" value="ruby" /> Ruby</label>
        </div>
        <div class="input-field">
          <label for="js-language-php"><input type="radio" id="js-language-php" class="js-language" name="language" value="php" /> PHP</label>
        </div>
        <div class="input-field">
          <label for="js-language-c"><input type="radio" id="js-language-c" class="js-language" name="language" value="c" /> C</label>
        </div>
        <div class="input-field">
          <label for="js-language-java"><input type="radio" id="js-language-java" class="js-language" name="language" value="java" /> Java</label>
        </div>
  	  </div>
  	  <div class="search-question">
  	    <div class="input-field">
  	      <label for="js-followers-0"><input type="radio" id="js-followers-0" class="js-followers" name="followers" value="0" /> 0 followers</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-followers-1-9"><input type="radio" id="js-followers-1-9" class="js-followers" name="followers" value="1..9" /> 1-9 followers</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-followers-10-49"><input type="radio" id="js-followers-10-49" class="js-followers" name="followers" value="10..49" /> 10-49 followers</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-followers-50-999"><input type="radio" id="js-followers-50-999" class="js-followers" name="followers" value="50..999" /> 50-999 followers</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-followers-1000"><input type="radio" id="js-followers-1000" class="js-followers" name="followers" value=">=1000" /> 1000+ followers</label>
  	    </div>
  	  </div>
	</div>
	<div class="js-non-tech-fields non-tech-fields">
	  	<div class="search-question">
  	    <div class="input-field">
  	      <label for="js-characteristics-css"><input type="radio" id="js-characteristics-css" class="js-characteristics" name="characteristics" value="css" /> Both a coder and estetic</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-characteristics-javascript"><input type="radio" id="js-characteristics-javascript" class="js-characteristics" name="characteristics" value="javascript" /> Someone that can make stuff in the browser move around</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-characteristics-ruby"><input type="radio" id="js-characteristics-ruby" class="js-characteristics" name="characteristics" value="ruby" /> A coder mod</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-characteristics-php"><input type="radio" id="js-characteristics-php" class="js-characteristics" name="characteristics" value="php" /> Classic web programmer</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-characteristics-c"><input type="radio" id="js-characteristics-c" class="js-characteristics" name="characteristics" value="c" /> A hard core code nerd</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-characteristics-java"><input type="radio" id="js-characteristics-java" class="js-characteristics" name="characteristics" value="java" /> A coder with an engineering degree</label>
  	    </div>
	    </div>
	    <div class="search-question">

  	    <div class="input-field">
  	      <label for="js-experience-0"><input type="radio" id="js-experience-0" class="js-experience" name="experience" value="0" /> Newb</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-experience-1-9"><input type="radio" id="js-experience-1-9" class="js-experience" name="experience" value="1-9" /> Up and coming</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-experience-10-49"><input type="radio" id="js-experience-10-49" class="js-experience" name="experience" value="10-49" /> Established</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-experience-50-999"><input type="radio" id="js-experience-50-999" class="js-experience" name="experience" value="50-999" /> Coder guru</label>
  	    </div>
  	    <div class="input-field">
  	      <label for="js-experience-1000"><input type="radio" id="js-experience-1000" class="js-experience" name="experience" value="1000" /> Coder superstar</label>
  	    </div>
	    </div>
	</div>
	<div class="form-submit">
    <div class="input-field location">
      <label for="js-location">Location:</label>
      <input type="text" id="js-location" class="js-location" placeholder="Anywhere" />
    </div>
    <input type="submit" value="Search" />
	</div>
  </form>
</section>
